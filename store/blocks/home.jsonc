  //Slider com os 2 Banners Principais do Site
  "slider-layout#banner": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "carousel"
    },
    "children": ["image#banner1", "image#banner2"]
  },

  "image#banner1": {
    "props": {
      "src": "https://github.com/thiagoteberga/upmedal-hiring-coders/blob/master/assets/img/banner.png?raw=true",
      "width": "100%",
      "maxWidth": "100%"
    }
  },
  "image#banner2": {
